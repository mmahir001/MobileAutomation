<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="iOS test suite on iPhone Simulator">

    <parameter name="OS" value="ios"/>
    <parameter name="appType" value="iPhone"/>
    <parameter name="deviceType" value="Simulator"/>
    <parameter name="deviceName" value="iPhone Simulator"/>
    <parameter name="version" value="12.2"/>
    <parameter name="appDir" value="app"/>
    <parameter name="appName" value="UICatalog6.1.app.zip"/>

    <test name="Test catalog page" preserve-order="true">
        <classes>
            <class name="tests.CatalogPageTest"/>
            <class name="tests.ButtonOpsTest"/>
            <class name="tests.TextFieldOpsTest"/>
        </classes>
    </test>
</suite>